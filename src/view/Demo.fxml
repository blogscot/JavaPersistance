<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.input.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="275.0" prefWidth="370.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="builder.Controller">
   <children>
      <Label layoutX="40.0" layoutY="65.0" text="Artist:" />
      <Label layoutX="40.0" layoutY="104.0" text="Album:" />
      <TextField fx:id="artistText" layoutX="85.0" layoutY="61.0" prefHeight="25.0" prefWidth="229.0" />
      <TextField fx:id="albumText" layoutX="85.0" layoutY="100.0" prefHeight="25.0" prefWidth="229.0" />
      <TextField fx:id="yearText" layoutX="85.0" layoutY="139.0" prefHeight="25.0" prefWidth="64.0" />
      <TextField fx:id="genreText" layoutX="85.0" layoutY="178.0" prefHeight="25.0" prefWidth="155.0" />
      <Button layoutX="136.0" layoutY="224.0" mnemonicParsing="false" onAction="#previous" text="&lt;&lt;" />
      <Button layoutX="201.0" layoutY="224.0" mnemonicParsing="false" onAction="#next" text="&gt;&gt;" />
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="370.0">
        <menus>
          <Menu text="_File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#load" text="Load">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="L" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#save" text="Save">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="X" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
            </items>
          </Menu>
          <Menu text="_Item">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#add" text="Add">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="A" control="DOWN" meta="UP" shift="DOWN" shortcut="UP" />
                     </accelerator></MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#delete" text="Delete">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="D" control="DOWN" meta="UP" shift="DOWN" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#clear" text="Clear">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="C" control="DOWN" meta="UP" shift="DOWN" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
               </items>
               <accelerator>
                  <KeyCodeCombination alt="UP" code="I" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
               </accelerator>
          </Menu>
          <Menu text="_Storage">
               <items>
                  <RadioMenuItem fx:id="fileIO" mnemonicParsing="false" selected="true" text="File IO">
                     <toggleGroup>
                        <ToggleGroup fx:id="Storage" />
                     </toggleGroup>
                  </RadioMenuItem>
                  <RadioMenuItem mnemonicParsing="false" onAction="#xml" text="XML" toggleGroup="$Storage" />
                  <RadioMenuItem mnemonicParsing="false" onAction="#serial" text="Serial" toggleGroup="$Storage" />
                  <RadioMenuItem mnemonicParsing="false" onAction="#sqlite" text="SQLite" toggleGroup="$Storage" />
               </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="42.0" layoutY="182.0" text="Genre:" />
      <Label layoutX="43.0" layoutY="143.0" text="Year:" />
   </children>
</AnchorPane>
